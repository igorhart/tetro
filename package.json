{
  "name": "tetro",
  "version": "0.2.0",
  "description": "Retro tetris game.",
  "author": "Igor Skuhar",
  "private": true,
  "bugs": "https://github.com/igorskuhar/tetro/issues",
  "repository": "https://github.com/igorskuhar/tetro.git",
  "license": "MIT",
  "engines": {
    "node": "8.11.2",
    "npm": "5.6.0"
  },
  "scripts": {
    "build:web": "webpack --config webpack.prod.js",
    "dev:server": "nodemon src/server/index.js",
    "dev:web": "webpack-serve --config webpack.dev.js",
    "heroku-postbuild": "cp -r build/web/. public",
    "lint:es": "eslint src",
    "lint:style": "stylelint 'src/**/*.css'",
    "postinstall": "npm test && npm run build:web",
    "precommit": "lint-staged",
    "start": "node src/server/index.js",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "browserslist": [
    "last 2 versions"
  ],
  "jest": {
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.js": "eslint",
    "*.json": "jsonlint",
    "*.css": "stylelint"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.5.1",
    "css-loader": "^0.28.11",
    "dotenv": "^6.0.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-import-resolver-babel-module": "^4.0.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.12.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-webpack-plugin": "^3.2.0",
    "html-webpack-template": "^6.2.0",
    "husky": "^0.14.3",
    "jest": "^23.1.0",
    "jsonlint-cli": "^1.0.1",
    "lint-staged": "^7.1.3",
    "mini-css-extract-plugin": "^0.4.0",
    "node-notifier": "^5.2.1",
    "nodemon": "^1.17.5",
    "optimize-css-assets-webpack-plugin": "^4.0.2",
    "style-loader": "^0.21.0",
    "stylelint": "^9.2.1",
    "stylelint-config-standard": "^18.2.0",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "webpack": "^4.10.2",
    "webpack-cli": "^3.0.1",
    "webpack-merge": "^4.1.2",
    "webpack-serve": "^1.0.2"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "express": "^4.16.3",
    "gsap": "^2.0.1",
    "morgan": "^1.9.0",
    "pg": "^7.4.3",
    "pg-hstore": "^2.3.2",
    "pixi-sound": "^2.0.2",
    "pixi.js": "^4.8.0",
    "sequelize": "^4.38.0"
  }
}
